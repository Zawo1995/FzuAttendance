{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "attendance": {
      "root": "",
      "sourceRoot": "src/main/webapp",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "less"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "src/main/webapp/dist",
            "index": "src/main/webapp/index.html",
            "main": "src/main/webapp/main.ts",
            "polyfills": "src/main/webapp/polyfills.ts",
            "tsConfig": "src/main/webapp/tsconfig.app.json",
            "assets": [
              "src/main/webapp/favicon.ico",
              "src/main/webapp/assets",
              "src/main/webapp/WEB-INF",
              "src/main/webapp/styles.css",
              "src/main/webapp/login",
              {
                "glob": "**/*",
                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "/assets/"
              }
            ],
            "styles": [
              "src/main/webapp/assets/vendor/bootstrap/css/bootstrap.css",
              "src/main/webapp/assets/vendor/font-awesome/css/font-awesome.css",
              "src/main/webapp/assets/vendor/magnific-popup/magnific-popup.css",
              "src/main/webapp/assets/vendor/bootstrap-datepicker/css/datepicker3.css",
              "src/main/webapp/assets/vendor/jstree/themes/default/style.css",
              "src/main/webapp/assets/vendor/jquery-ui/css/ui-lightness/jquery-ui-1.10.4.custom.css",
              "src/main/webapp/assets/vendor/bootstrap-multiselect/bootstrap-multiselect.css",
              "src/main/webapp/assets/vendor/morris/morris.css",
              "src/main/webapp/assets/stylesheets/theme.css",
              "src/main/webapp/assets/vendor/select2/select2.css",
              "src/main/webapp/assets/vendor/select2/select2-bootstrap.css",
              "src/main/webapp/assets/stylesheets/skins/default.css",
              "src/main/webapp/assets/stylesheets/theme-custom.css",
              "src/main/webapp/assets/vendor/pnotify/pnotify.custom.css",
              "src/main/webapp/assets/layui/css/layui.css",
              "src/main/webapp/assets/vendor/bootstrap-datetimepicker/css/bootstrap-datetimepicker.css",
              "src/main/webapp/styles.css",
              "src/main/webapp/assets/layui/checkbox/checkbox.css",
              "src/main/webapp/assets/Xenon/css/fonts/linecons/css/linecons.css",
              "src/main/webapp/assets/Xenon/css/fonts/fontawesome/css/font-awesome.min.css",
              "src/main/webapp/assets/Xenon/css/bootstrap.css",
              "src/main/webapp/assets/Xenon/css/xenon-core.css",
              "src/main/webapp/assets/Xenon/css/xenon-forms.css",
              "src/main/webapp/assets/Xenon/css/xenon-components.css",
              "src/main/webapp/assets/Xenon/css/xenon-skins.css",
              "src/main/webapp/assets/Xenon/css/custom.css",
              "node_modules/ng-zorro-antd/ng-zorro-antd.min.css"
            ],
            "scripts": [
              "src/main/webapp/assets/my/config/config.js",
              "src/main/webapp/assets/vendor/modernizr/modernizr.js",
              "src/main/webapp/assets/vendor/jquery/jquery.js",
              "src/main/webapp/assets/vendor/jquery-browser-mobile/jquery.browser.mobile.js",
              "src/main/webapp/assets/vendor/bootstrap/js/bootstrap.js",
              "src/main/webapp/assets/vendor/nanoscroller/nanoscroller.js",
              "src/main/webapp/assets/vendor/magnific-popup/magnific-popup.js",
              "src/main/webapp/assets/vendor/jquery-placeholder/jquery.placeholder.js",
              "src/main/webapp/assets/vendor/bootstrap-datepicker/js/bootstrap-datepicker.js",
              "src/main/webapp/assets/vendor/bootstrap-timepicker/js/bootstrap-timepicker.js",
              "src/main/webapp/assets/vendor/bootstrap-datetimepicker/js/bootstrap-datetimepicker.js",
              "src/main/webapp/assets/vendor/jquery-ui/js/jquery-ui-1.10.4.custom.js",
              "src/main/webapp/assets/vendor/jquery-ui-touch-punch/jquery.ui.touch-punch.js",
              "src/main/webapp/assets/vendor/jquery-appear/jquery.appear.js",
              "src/main/webapp/assets/vendor/bootstrap-multiselect/bootstrap-multiselect.js",
              "src/main/webapp/assets/vendor/jquery-easypiechart/jquery.easypiechart.js",
              "src/main/webapp/assets/vendor/flot/jquery.flot.js",
              "src/main/webapp/assets/vendor/flot-tooltip/jquery.flot.tooltip.js",
              "src/main/webapp/assets/vendor/flot/jquery.flot.pie.js",
              "src/main/webapp/assets/vendor/flot/jquery.flot.categories.js",
              "src/main/webapp/assets/vendor/flot/jquery.flot.resize.js",
              "src/main/webapp/assets/vendor/jquery-sparkline/jquery.sparkline.js",
              "src/main/webapp/assets/vendor/raphael/raphael.js",
              "src/main/webapp/assets/vendor/morris/morris.js",
              "src/main/webapp/assets/vendor/gauge/gauge.js",
              "src/main/webapp/assets/vendor/snap-svg/snap.svg.js",
              "src/main/webapp/assets/vendor/liquid-meter/liquid.meter.js",
              "src/main/webapp/assets/vendor/bootstrap-fileupload/bootstrap-fileupload.min.js",
              "src/main/webapp/assets/javascripts/theme.js",
              "src/main/webapp/assets/javascripts/theme.custom.js",
              "src/main/webapp/assets/javascripts/theme.init.js",
              "src/main/webapp/assets/vendor/select2/select2.min.js",
              "src/main/webapp/assets/vendor/pnotify/pnotify.custom.js",
              "src/main/webapp/assets/my/boostrap-nav.js",
              "src/main/webapp/assets/vendor/jstree/jstree.js",
              "src/main/webapp/assets/vendor/jquery-qrcode/qrcode.js",
              "src/main/webapp/assets/vendor/jquery-qrcode/jquery.qrcode.js",
              "src/main/webapp/assets/my/print/printarea.js",
              "src/main/webapp/assets/my/print/printAreaConfig.js",
              "src/main/webapp/assets/Xenon/js/bootstrap.min.js",
              "src/main/webapp/assets/Xenon/js/TweenMax.min.js",
              "src/main/webapp/assets/Xenon/js/resizeable.js",
              "src/main/webapp/assets/Xenon/js/joinable.js",
              "src/main/webapp/assets/Xenon/js/xenon-api.js",
              "src/main/webapp/assets/Xenon/js/xenon-toggles.js",
              "src/main/webapp/assets/Xenon/js/xenon-custom.js",
              "src/main/webapp/assets/Xenon/js/jquery-1.11.1.min.js",
              "src/main/webapp/assets/Xenon/js/html5shiv.min.js",
              "src/main/webapp/assets/Xenon/js/respond.min.js"
            ],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/main/webapp/environments/environment.ts",
                  "with": "src/main/webapp/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "attendance:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "attendance:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "attendance:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/main/webapp/test.ts",
            "polyfills": "src/main/webapp/polyfills.ts",
            "tsConfig": "src/main/webapp/tsconfig.spec.json",
            "karmaConfig": "src/main/webapp/karma.conf.js",
            "styles": [
              "src/main/webapp/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/main/webapp/favicon.ico",
              "src/main/webapp/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/main/webapp/tsconfig.app.json",
              "src/main/webapp/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "attendance-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "attendance:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "attendance:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "attendance"
}
